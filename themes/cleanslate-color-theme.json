{
	"name": "cleanslate",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#111",
		"editor.foreground": "#eee",
		"activityBarBadge.background": "#c55c5c",
		"sideBarTitle.foreground": "#bbbbbb",
		"gitDecoration.addedResourceForeground": "#81a584",
		"gitDecoration.conflictingResourceForeground": "#aa93a1",
		"gitDecoration.ignoredResourceForeground": "#999",
		"gitDecoration.modifiedResourceForeground": "#ceab6f",
		"gitDecoration.deletedResourceForeground": "#c55c5c",
		"gitDecoration.stageDeletedResourceForeground": "#c55c5c",
		"gitDecoration.stageModifiedResourceForeground": "#ceab6f",
		"gitDecoration.untrackedResourceForeground": "#8eb2ca"
	},
	"tokenColors": [
		{
			"name": "Regular text",
			"scope": [
				"meta.embedded variable.other",
				"punctuation.accessor",
				"meta.function-call",
				"constant"
			],
			"settings": {
				"foreground": "#eee",
				"fontStyle": ""
			}
		},
		{
			"name": "Constant, special variable",
			"scope": [
				"constant.language",
				"variable.language.this",
				"support.type.builtin.graphql",
				"support.type.location.graphql",
				"constant.character.entity"
			],
			"settings": {
				"foreground": "#42a08b",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#81a584"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string",
				"meta.attribute-selector.scss"
			],
			"settings": {
				"foreground": "#db9482"
			}
		},
		{
			"name": "Interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"variable.interpolation.scss punctuation.definition",
				"constant.character.format.placeholder"
			],
			"settings": {
				"foreground": "#c55c5c",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"keyword.operator.logical.python",
				"keyword.operator.new",
				"keyword.type.graphql",
				"keyword.scalar.graphql",
				"keyword.enum.graphql",
				"keyword.interface.graphql",
				"keyword.directive.graphql",
				"keyword.on.graphql",
				"storage"
			],
			"settings": {
				"foreground": "#8eb2ca"
			}
		},
		{
			"name": "Key",
			"scope": [
				"meta.object-literal.key",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#ceab6f"
			}
		},
		{
			"name": "Class, function and method declaration",
			"scope": [
				"meta.definition.function",
				"meta.definition.method",
				"meta.class entity.name.type",
				"entity.name.function.python",
				"support.type.graphql",
				"support.type.enum.graphql",
				"entity.scalar.graphql",
				"entity.name.function.directive.graphql"
			],
			"settings": {
				"foreground": "#dfcb94",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Python function signature",
			"scope": [
				"meta.function.parameters.python",
				"variable.parameter.function.language.special.self.python"
			],
			"settings": {
				"foreground": "#999",
			}
		},
		{
			"name": "Function parameter declaration",
			"scope": [
				"meta.function variable.parameter",
				"meta.method.declaration variable.parameter",
				"variable.parameter.graphql"
			],
			"settings": {
				"foreground": "#ceab6f"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.logical",
				"keyword.operator.relational",
				"keyword.operator.comparison",
				"keyword.operator.ternary",
				"keyword.operator.nulltype.graphql",
				"punctuation.assignment.graphql"
			],
			"settings": {
				"foreground": "#b3a28d"
			}
		},
		{
			"name": "Named parameter in function call",
			"scope": [
				"meta.function-call variable.parameter"
			],
			"settings": {
				"foreground": "#ceab6f",
				"fontStyle": ""
			}
		},
		{
			"name": "CSS id rule",
			"scope": [
				"entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#dfcb94",
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS class rule",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#aa93a1",
				"fontStyle": "bold"
			}
		},
		{
			"name": "SCSS variable",
			"scope": [
				"variable.scss"
			],
			"settings": {
				"foreground": "#a3937e",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Type declaration",
			"scope": [
				"meta.type.object.graphql support.type.graphql"
			],
			"settings": {
				"foreground": "#9b86b4",
				"fontStyle": ""
			}
		},
		{
			"name": "Block delimiter, punctuation",
			"scope": [
				"punctuation.definition",
				"punctuation.terminator",
				"punctuation.parenthesis",
				"punctuation.separator",
				"punctuation.section.property-list",
				"meta.brace",
				"punctuation.operation.graphql"
			],
			"settings": {
				"foreground": "#999"
			}
		},
		{
			"name": "XML preprocessor",
			"scope": [
				"entity.name.tag.xml",
				"meta.tag.sgml.doctype"
			],
			"settings": {
				"foreground": "#a3937e",
				"fontStyle": "bold"
			}
		},
		{
			"name": "XML preprocessor, SGML doctype",
			"scope": [
				"meta.tag.preprocessor.xml entity.other.attribute-name.xml"
			],
			"settings": {
				"foreground": "#ceab6f"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#8d5576",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Tag attribute, React JSX attribute value delimiter",
			"scope": [
				"entity.other.attribute-name",
				"punctuation.section.embedded.begin.js.jsx",
				"punctuation.section.embedded.end.js.jsx"
			],
			"settings": {
				"foreground": "#9b86b4",
			}
		},
		{
			"name": "Tag punctuation",
			"scope": [
				"punctuation.definition.tag",
				"meta.tag keyword.operator.assignment",
				"meta.attribute-selector.scss keyword.operator.scss"
			],
			"settings": {
				"foreground": "#4d4058",
			}
		},
		{
			"name": "Tag namespace",
			"scope": [
				"entity.name.tag.namespace"
			],
			"settings": {
				"foreground": "#aa93a1",
				"fontStyle": "bold"
			}
		},
		{
			"name": "React component tag",
			"scope": [
				"support.class.component"
			],
			"settings": {
				"foreground": "#aa93a1",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Decorator",
			"scope": [
				"entity.name.function.decorator",
				"meta.function.decorator",
			],
			"settings": {
				"foreground": "#999",
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
	],
	"semanticTokenColors": {
		"*.typeHint:python": {
			"foreground": "#9b86b4"
		}
	}
}